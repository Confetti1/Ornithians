<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <TerrainDef ParentName="FloorBase">
    <defName>StickweaveFloor</defName>
    <label>Stickweave</label>
    <description>It's ugly, but it's good for slowing down invaders. It is also quite flammable.</description>
    <texturePath>Terrain/Stickweave</texturePath>
    <burnedDef>BurnedStickweaveFloor</burnedDef>
    <edgeType>Hard</edgeType>
    <researchPrerequisites>
      <li>OrnithianResearch</li>
    </researchPrerequisites>
    <renderPrecedence>80</renderPrecedence>
    <pathCost>12</pathCost>
    <statBases>
      <Beauty>-2</Beauty>
      <WorkToBuild>300</WorkToBuild>
      <Flammability>0.4</Flammability>
    </statBases>
    <costList>
      <WoodLog>2</WoodLog>
    </costList>
    <constructEffect>ConstructWood</constructEffect>
    <designationHotKey>Misc9</designationHotKey>
  </TerrainDef>
  
  <TerrainDef ParentName="BurnedFloorBase">
    <defName>BurnedStickweaveFloor</defName>
    <label>burned stickweave</label>
    <renderPrecedence>81</renderPrecedence>
    <description>Burnt up woven sticks. Mostly ashes now.</description>
    <texturePath>Terrain/StickweaveBurned</texturePath>
    <edgeType>FadeRough</edgeType>
  </TerrainDef>
    
</Defs>
