<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <BodyDef>
    <defName>KneaderpillarBody</defName>
    <label>kneaderpillar</label>
    <corePart>
      <def>Abdomen</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <!-- Midgut (stomach), intestine, six legs -->
        <li>
          <def>Stomach</def>
          <coverage>0.03</coverage>
          <customLabel>midgut</customLabel>
          <depth>Inside</depth>
        </li>
        <li>
          <def>DorsalVessel</def>
          <coverage>0.03</coverage>
          <customLabel>dorsal blood vessel</customLabel>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Kidney</def>
          <customLabel>left kidney</customLabel>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Kidney</def>
          <customLabel>right kidney</customLabel>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Liver</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>KneaderpillarLeg</def>
          <customLabel>left false leg 1</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>KneaderpillarLeg</def>
          <customLabel>right false leg 1</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>KneaderpillarLeg</def>
          <customLabel>left false leg 2</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>KneaderpillarLeg</def>
          <customLabel>right false leg 2</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>KneaderpillarLeg</def>
          <customLabel>left false leg 3</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>KneaderpillarLeg</def>
          <customLabel>right false leg 3</customLabel>
          <coverage>0.07</coverage>
          <height>Bottom</height>
        </li>
        <li>
          <def>Thorax</def>
          <height>Middle</height>
          <depth>Outside</depth>
          <parts>
            <li>
              <def>Head</def>
              <coverage>0.5</coverage>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>Skull</def>
                  <customLabel>head capsule</customLabel>
                  <coverage>0.25</coverage>
                  <depth>Inside</depth>
                  <parts>
                    <li>
                      <def>Brain</def>
                      <coverage>0.70</coverage>
                      <depth>Inside</depth>
                    </li>
                  </parts>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>left eye</customLabel>
                  <coverage>0.12</coverage>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>right eye</customLabel>
                  <coverage>0.12</coverage>
                </li>
                <li>
                  <def>Lung</def>
                  <customLabel>left air sac</customLabel>
                  <coverage>0.7</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                  <def>Lung</def>
                  <customLabel>right air sac</customLabel>
                  <coverage>0.7</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                  <def>AnimalJaw</def>
                  <coverage>0.10</coverage>
                  <groups>
                    <li>Teeth</li>
                  </groups>
                </li>
              </parts>
            </li>
            <li>
              <def>KneaderpillarLeg</def>
              <customLabel>front left leg</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
            </li>
            <li>
              <def>KneaderpillarLeg</def>
              <customLabel>front right leg</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
            </li>
            <li>
              <def>KneaderpillarLeg</def>
              <customLabel>back left leg</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
            </li>
            <li>
              <def>KneaderpillarLeg</def>
              <customLabel>back right leg</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>
        
        <!-- four legs, esophagus, head (Mouth, brain, eyes) -->
</Defs>
