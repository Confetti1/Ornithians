<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="PlantBase">
    <defName>Plant_Millet</defName>
    <label>millet plant</label>
    <description>A plant known for it's abundance of seeds, which Ornithians find delicious. Humans can eat it too, but won't enjoy it raw like Ornithians.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Nutrition>0.2</Nutrition>
    </statBases>
    <sowResearchPrerequisites>
        <li>OrnithianResearch</li>
      </sowResearchPrerequisites>
    <graphicData>
      <texPath></texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>14</pathCost>
    <ingestible />
    <plant>
      <dieIfLeafless>true</dieIfLeafless>
      <fertilitySensitivity>0.2</fertilitySensitivity>
      <maxMeshCount>4</maxMeshCount>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>RawMillet</harvestedThingDef>
      <harvestYield>5</harvestYield>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <topWindExposure>0.1</topWindExposure>
      <growDays>3</growDays>
      <visualSizeRange>0.3~0.8</visualSizeRange>
      <wildOrder>2</wildOrder>
      <purpose>Food</purpose>
    </plant>
  </ThingDef>
</Defs>
